generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id       String @id @default(uuid())
  name     String?
  email    String @unique(map: "User_email_key")
  password String
  
}

model task {
  id          String  @id
  title       String
  isCompleted Boolean @default(false)
  userId      String

}